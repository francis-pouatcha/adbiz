<?xml version="1.0" encoding="UTF-8"?>

<!-- Wildfly module for b2 persistence store for keycloak-->
<module xmlns="urn:jboss:module:1.3" name="org.adorsys.adkcloak.connections.b2">
    <resources>
        <resource-root path="adkcloak.connections.b2.jar"/>
    </resources>

    <exports>
        <include path="META-INF/**"/>
    </exports>
    <dependencies>
        <module name="org.keycloak.keycloak-common"/>
        <module name="org.keycloak.keycloak-core"/>
        <module name="org.keycloak.keycloak-model-api"/>
        <module name="org.keycloak.keycloak-connections-jpa"/>
        <module name="javax.persistence.api"/>
        <module name="org.jboss.logging"/>
        <module name="org.bouncycastle" />
        <module name="javax.api"/>
        <module name="org.hibernate" services="import"/>
        <module name="org.javassist"/>
    </dependencies>
</module>

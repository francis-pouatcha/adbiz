<?xml version="1.0" encoding="UTF-8"?>

<!-- Wildfly module for b2 persistence store for keycloak-->
<module xmlns="urn:jboss:module:1.3" name="org.adorsys.adkcloak.cache.none">
    <resources>
        <resource-root path="adkcloak.cache.none.jar"/>
    </resources>

    <exports>
        <include path="META-INF/**"/>
    </exports>
    <dependencies>
        <module name="org.keycloak.keycloak-common"/>
        <module name="org.keycloak.keycloak-core"/>
        <module name="org.keycloak.keycloak-model-api"/>
    	<module name="org.keycloak.keycloak-invalidation-cache-model"/>
    </dependencies>
</module>

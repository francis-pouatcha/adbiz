            <method name="beforeOpen"><![CDATA[var rayon = reportContext.getHttpServletRequest().getParameter('rayon');
this.queryText = "SELECT * FROM(SELECT STKSECTION.IDENTIF AS ID_RAYON, STKSECTION.NAME  AS RAYON, CATALARTICLE.ID AS CIP , CATALARTLANGMAPPING.ARTNAME AS DESIGNATION , STKARTICLELOT.TRGTQTY AS STOCK , STKARTICLELOT.PRCHUNITPRCTAXINCL AS PA , CATALARTICLE.SPPU AS PV, CATALARTICLE.VATRATE AS TVA, CATALARTICLE.MINSTOCKQTY AS MINQTY FROM STKSECTION INNER JOIN STKARTICLELOT ON STKARTICLELOT.SECTION = STKSECTION.IDENTIF INNER JOIN CATALARTICLE ON CATALARTICLE.ID  = STKARTICLELOT.ARTPIC INNER JOIN CATALARTLANGMAPPING ON CATALARTLANGMAPPING.CNTNRIDENTIF = STKARTICLELOT.ARTPIC ORDER BY CATALARTLANGMAPPING.ARTNAME) WHERE ID_RAYON = '"+rayon+"'";]]></method>

            <xml-property name="queryText"><![CDATA[SELECT CATALARTICLE.ID AS CIP , 
CATALARTLANGMAPPING.ARTNAME AS DESIGNATION , 
STKARTICLELOT.TRGTQTY AS STOCK , 
STKARTICLELOT.PRCHUNITPRCTAXINCL AS PA , 
CATALARTICLE.SPPU AS PV, 
CATALARTICLE.VATRATE AS TVA, 
CATALARTICLE.MINSTOCKQTY AS MINQTY
FROM CATALARTICLE 
INNER JOIN CATALARTLANGMAPPING  
ON CATALARTICLE.ID  = CATALARTLANGMAPPING.CNTNRIDENTIF 
LEFT JOIN STKARTICLELOT 
ON CATALARTICLE.ID = STKARTICLELOT.ARTPIC
ORDER BY CATALARTLANGMAPPING.ARTNAME]]></xml-property>

            <method name="beforeOpen"><![CDATA[var param = reportContext.getHttpServletRequest().getParameter("rayon");
this.queryText = "SELECT CATALARTICLE.ID AS CIP,STKSECTION.NAME AS RAYON, STKARTICLELOT.TRGTQTY  AS STOCK, STKARTICLELOT.PRCHUNITPRCTAXINCL AS PA, CATALARTICLE.SPPU AS PV, CATALARTLANGMAPPING.ARTNAME AS DESIGNATION, FAMCODE AS FAMILLE FROM CATALARTICLE INNER JOIN CATALARTLANGMAPPING ON CATALARTICLE.ID = CATALARTLANGMAPPING.CNTNRIDENTIF LEFT JOIN STKARTICLELOT ON CATALARTICLE.ID = STKARTICLELOT.ARTPIC INNER JOIN STKSECTION ON STKARTICLELOT.SECTION = STKSECTION.ID RIGHT JOIN CATALART2PRODUCTFAMILY ON CATALARTICLE.ID = CATALART2PRODUCTFAMILY.ARTPIC WHERE STKARTICLELOT.STKGDT BETWEEN '"+vars['debut']+"' AND '"+vars['fin']+"' AND STKARTICLELOT.SECTION = '"+param+"'";]]></method>
